{
  "expo": {
    "name": "Crossword",
    "slug": "ContactsCrossword",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.20",
    "orientation": "portrait",
    "icon": "./assets/contactsCrosswordIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.willdavenport.contactscrossword",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6643827733570457~1971802195"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": "NO",
        "NSContactsUsageDescription": "Contacts Crosswords uses your contacts list to create the crossword puzzles for you to play. We will not share your data."
      }
    },
    "description": ""
  }
}
